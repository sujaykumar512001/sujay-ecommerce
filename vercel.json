{
  "version": 2,
  "name": "sujay-ecommerce-test",
  "builds": [
    {
      "src": "api/hello.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/hello.js"
    }
  ],
  "functions": {
    "api/hello.js": {
      "maxDuration": 10
    }
  }
} 